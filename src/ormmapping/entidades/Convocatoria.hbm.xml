<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entidades.Convocatoria" table="convocatoria" schema="public" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">convocatoria_id_seq</param>
			</generator>
		</id>
		<many-to-one name="id_estudiante" cascade="lock" column="id_estudiante" class="entidades.Estudiante" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="id_asignatura" cascade="lock" column="id_asignatura" class="entidades.Asignatura" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="fecha" column="fecha" type="date" not-null="true" lazy="false"/>
		<property name="nota" column="nota" type="double" length="10" not-null="true" lazy="false"/>
		<property name="numero" column="numero" type="integer" length="10" not-null="true" lazy="false"/>
		<set name="ORM_Nota_convocatoria" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="id_convocatoria" not-null="true"/>
			<one-to-many class="entidades.Nota_convocatoria"/>
		</set>
	</class>
</hibernate-mapping>
