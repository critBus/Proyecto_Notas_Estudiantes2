<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entidades.Estudiante" table="estudiante" schema="public" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">estudiante_id_seq</param>
			</generator>
		</id>
		<property name="nombre" column="nombre" type="string" length="50" not-null="true" lazy="false"/>
		<property name="apellidos" column="apellidos" type="string" length="50" not-null="true" lazy="false"/>
		<property name="grupo" column="grupo" type="string" length="256" not-null="true" lazy="false"/>
		<set name="ORM_Asignatura_estudiante" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="id_estudiante" not-null="true"/>
			<one-to-many class="entidades.Asignatura_estudiante"/>
		</set>
		<set name="ORM_Convocatoria" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="id_estudiante" not-null="true"/>
			<one-to-many class="entidades.Convocatoria"/>
		</set>
	</class>
</hibernate-mapping>
