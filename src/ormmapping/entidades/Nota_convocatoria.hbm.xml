<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entidades.Nota_convocatoria" table="nota_convocatoria" schema="public" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">nota_convocatoria_id_seq</param>
			</generator>
		</id>
		<many-to-one name="id_convocatoria" cascade="lock" column="id_convocatoria" class="entidades.Convocatoria" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nota_final" column="nota_final" type="double" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
