<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entidades.Carrera" table="carrera" schema="public" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">carrera_id_seq</param>
			</generator>
		</id>
		<many-to-one name="id_facultad" cascade="lock" column="id_facultad" class="entidades.Facultad" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="carrera" column="carrera" type="string" length="50" not-null="true" lazy="false"/>
		<set name="ORM_Asignatura" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="id_carrera" not-null="true"/>
			<one-to-many class="entidades.Asignatura"/>
		</set>
	</class>
</hibernate-mapping>
