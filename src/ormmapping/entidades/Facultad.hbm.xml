<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entidades.Facultad" table="facultad" schema="public" lazy="false">
		<id name="id" column="id" type="integer" unsaved-value="0">
			<generator class="increment">
				<param name="schema">public</param>
			</generator>
		</id>
		<property name="facultad" column="facultad" type="string" length="50" not-null="true" lazy="false"/>
		<set name="ORM_Carrera" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="id_facultad" not-null="true"/>
			<one-to-many class="entidades.Carrera"/>
		</set>
	</class>
</hibernate-mapping>
